{
  "expo": {
    "name": "YoutubeClone",
    "slug": "YoutubeClone",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.SaurabhCo.YouTubeClone",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.SaurabhCo.YouTubeClone",
      "versionCode": 1
    },
    "description": "Simple youtube clone using react native",
    "githubUrl": "https://github.com/Saurabh2699"
  }
}
